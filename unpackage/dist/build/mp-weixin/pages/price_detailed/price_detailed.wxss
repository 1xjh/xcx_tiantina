.price_detailed_list {
  width: 95%;
  margin: 0 auto;
  font-family: Source Han Sans CN;
  font-weight: 400;
}
.price_detailed_list .price_detailed_list_title {
    margin: 72rpx 0rpx 28rpx 0rpx;
    font-size: 30rpx;
    margin-left: 32rpx;
    font-weight: bold;
    color: #267656;
}
.price_detailed_list .room_name {
    font-size: 30rpx;
    color: black;
}
.price_detailed_list .cu-list {
    border-radius: 20rpx;
}
.price_detailed_list .text-sm {
    font-size: 24rpx;
    color: #555555;
}
.price_detailed_list .price_font {
    font-size: 32rpx;
    font-weight: bold;
    color: #ff3737;
}
.price_detailed_list .list_date {
    font-size: 24rpx;
    color: #555555;
    background: #FFFFFF !important;
}

