	font-size: 30rpx;
}
.residence_box text {
	font-size: 24rpx;
	color: rgba(95, 95, 95, 1);
	margin: 25rpx 0rpx 25rpx 0rpx;
}
.residence_end,
.residence_start {
	display: -webkit-box;
	display: -webkit-flex;
