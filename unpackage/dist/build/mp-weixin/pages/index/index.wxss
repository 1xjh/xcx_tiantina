.swiper {
	width: 100%;
	height: 500rpx;
	border-radius: 0 0 240px 50%/20px;
	overflow: hidden;
}
.swiper swiper,
.itemSwiper swiper {
	width: 100%;
	height: 100%;
}
.itemSwiper {
	margin-bottom: 10rpx;
	border-bottom: 1px solid #efeff7;
	margin-top: 15px;
}
image {
	width: 100%;
	height: 100%;
	display: block;
}
.search_icon image {
	display: inline-block;
	width: 14px;
	height: 12px;
}
.residence_title {
	height: 90rpx;
	overflow: hidden;
	line-height: 90rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.residence_title image {
	height: 24rpx;
	width: 24rpx;
	margin-top: 33rpx;
	margin-right: -6px;
}
.residence_title text {
	font-size: 20rpx;
}
.residence_title>view {
	font-size: 36rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-family: SourceHanSansCN-Medium;
	font-weight: bold;
	color: rgba(43, 43, 43, 1);
}
.residence_title>view text {
	font-size: 24rpx;
	font-family: SourceHanSansCN-Regular;
	font-weight: 400;
	color: rgba(85, 85, 85, 1);
	margin-left: 20rpx;
}
/* 时间 */
.residence_day {
	font-size: 24rpx;
	color: #2b2b2b;
	width: 90rpx;
	height: 40rpx;
	text-align: center;
	line-height: 40rpx;
	border: 1px solid rgba(204, 204, 204, 1);
	border-radius: 20px;
}
.residence {
	width: 80%;
	margin: 30rpx auto;
	-webkit-box-shadow: 1rpx 2rpx 30rpx #888;
	        box-shadow: 1rpx 2rpx 30rpx #888;
	border-radius: 20rpx;
	margin-top: -90rpx;
	position: relative;
	background: #fff;
	padding: 20rpx 40rpx;
}
.residence_box {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
