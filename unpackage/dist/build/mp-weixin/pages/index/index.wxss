.residence_end text {
	text-align: right;
}
/* 横线 */
.line {
	height: 1px;
	width: 100%;
	background: #eee;
}
.search_text {
	-webkit-box-shadow: 0rpx 10rpx 10rpx #eee;
	        box-shadow: 0rpx 10rpx 10rpx #eee;
