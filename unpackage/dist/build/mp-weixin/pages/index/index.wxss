	line-height: 91rpx;
	letter-spacing: 5rpx;
}
/* 地区图区 */
.region {
	width: 90%;
	margin: 0 auto;
}
.region_name {
	z-index: 5;
	color: #fff;
	position: absolute !important;
	right: 0;
	bottom: 0;
	margin: 20rpx;
	font-size: 36rpx;
}
.region .bg-img {
	margin-top: 40rpx;
}
.index_bottom {
	margin: 21rpx auto;
	width: 90%;
	z-index: 100;
}
.index_bottom .index_bottom_name {
	width: 23%;
	margin: 20rpx auto 20rpx auto;
	height: 62rpx;
}
.index_bottom .index_bottom_phone {
	width: 31.7%;
	margin: 0 auto;
	height: 16rpx;
}
