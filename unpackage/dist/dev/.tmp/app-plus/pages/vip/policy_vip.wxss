page {
  background: #FFFFFF;
}
.policy_vip_box {
  width: 93%;
  font-family: Source Han Sans CN;
  margin: 0 auto;
}
.policy_vip_grade {
  margin: 68rpx 0rpx 36rpx 0rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #555555;
}
.policy_vip_grade_list_content {
  font-size: 26rpx;
}
.policy_vip_grade_list_content .vip-sub {
    background: #fbfafa;
    text-align: center;
    height: 160rpx;
    line-height: 160rpx;
    margin-left: 5rpx;
    color: #555555;
}
.policy_vip_grade_list_content .vip_name {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
        -ms-flex: 1;
            flex: 1;
    margin-left: 0;
    color: #30926b;
}
.policy_vip_grade_list_content .vip_zhe {
    -webkit-box-flex: 2;
    -webkit-flex: 2;
        -ms-flex: 2;
            flex: 2;
}
.policy_vip_grade_list_content .vip_date {
    -webkit-box-flex: 2;
    -webkit-flex: 2;
        -ms-flex: 2;
            flex: 2;
}
.policy_vip_grade_list {
  font-size: 26rpx;
}
.policy_vip_grade_list .vip-sub {
    background: #267656;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    color: #FFFFFF;
    margin-left: 5rpx;
}
.policy_vip_grade_list .vip_name {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
        -ms-flex: 1;
            flex: 1;
    margin-left: 0;
    color: #FFFFFF;
    border-radius: 10px 0px 0px 0px;
}
.policy_vip_grade_list .vip_zhe {
    -webkit-box-flex: 2;
    -webkit-flex: 2;
        -ms-flex: 2;
            flex: 2;
}
.policy_vip_grade_list .vip_date {
    border-radius: 0px 10px 0px 0px;
    -webkit-box-flex: 2;
    -webkit-flex: 2;
        -ms-flex: 2;
            flex: 2;
}
.policy_vip_title {
  background: #f6f5f5;
  border-radius: 20px;
  position: relative;
  margin-top: 70rpx;
}
.policy_vip_title .plicy_top_content {
    padding: 70rpx 40rpx 22rpx 40rpx;
    font-size: 28rpx;
    font-family: Source Han Sans CN;
    font-weight: 500;
    color: #555555;
}
.policy_vip_title .title_top {
    height: 68rpx;
    line-height: 68rpx;
    width: 80%;
    left: 0;
    right: 0;
    text-align: center;
    margin: 0 auto;
    top: -34rpx;
    background: #267656;
    border-radius: 10rpx;
    position: absolute;
    background: #267656;
    font-size: 32rpx;
    font-family: Source Han Sans CN;
    font-weight: 500;
    color: white;
}

