<view class="sort_price"><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">{{city}}</view></cu-custom><view class="slider"><view class="title">价格区间</view><view class="title">不限</view><slider-range vue-id="2" value="{{rangeValue}}" min="{{rangeMin}}" max="{{rangMax}}" step="{{5}}" bar-height="{{3}}" block-size="{{26}}" background-color="#999999" active-color="#267656" format="{{format}}" decorationVisible="{{true}}" data-event-opts="{{[['^change',[['handleRangeChange']]]]}}" bind:change="__e" bind:__l="__l"></slider-range><view class="nav-list"><view>{{"￥"+rangeValue[0]}}</view><view>{{"￥"+(rangeValue[1]==rangMax?rangeValue[1]+'以上':rangeValue[1])}}</view></view><view class="select_price"><view data-event-opts="{{[['tap',[['select_price',[0,400]]]]]}}" class="price" bindtap="__e">￥0-400</view><view data-event-opts="{{[['tap',[['select_price',[400,800]]]]]}}" class="price" bindtap="__e">￥400~800</view><view data-event-opts="{{[['tap',[['select_price',[800,1200]]]]]}}" class="price" bindtap="__e">￥800~1200</view><view data-event-opts="{{[['tap',[['select_price',[1200,1600]]]]]}}" class="price" bindtap="__e">￥1200~1600</view><view data-event-opts="{{[['tap',[['select_price',[1600,2000]]]]]}}" class="price" bindtap="__e">￥1600~2000</view><view data-event-opts="{{[['tap',[['select_price',[0,2000]]]]]}}" class="price" bindtap="__e">2000以上</view></view></view></view>