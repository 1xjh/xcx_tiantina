.withdraw_main {
  width: 90%;
  margin: 40rpx auto;
  font-family: Source Han Sans CN;
}
.withdraw_main .widthdraw_icon {
    width: 36rpx;
    height: 30rpx;
    background: #007AFF;
}
.withdraw_main .submit_price {
    font-size: 28rpx;
    margin: 88rpx 0rpx 60rpx 0rpx;
    font-weight: 400;
    color: black;
}
.withdraw_main .import_price {
    font-size: 52rpx;
    font-weight: bold;
    color: black;
    padding: 20rpx 0rpx 80rpx 0rpx;
    border-bottom: 1px solid #eee;
}
.withdraw_main .account_right {
    margin-left: 67rpx;
}
.withdraw_main .bottom_sumbit {
    width: 100%;
    height: 96rpx;
    text-align: center;
    font-size: 36rpx;
    font-weight: bold;
    color: white;
    border-radius: 10rpx;
    line-height: 96rpx;
    margin-top: 111rpx;
}
.withdraw_main .widthdraw_card {
    background: #FFFFFF;
    border-radius: 20rpx;
    padding: 44rpx 50rpx 70rpx 50rpx;
}
.withdraw_main .widthdraw_card .card_account {
      font-size: 28rpx;
      font-weight: 400;
      color: black;
}
.withdraw_main .widthdraw_card .account_day {
      font-size: 28rpx;
      font-weight: 400;
      color: #555555;
      margin-top: 22rpx;
}
.withdraw_main .widthdraw_card .card_account_info {
      margin-left: 8rpx;
}
