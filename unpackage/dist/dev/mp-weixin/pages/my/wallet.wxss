.wallet_bg {
  width: 100%;
  height: 382rpx;
  position: relative;
  background: -webkit-gradient(linear, left bottom, left top, from(#267656), to(rgba(92, 187, 148, 0.05)));
  background: -o-linear-gradient(bottom, #267656, rgba(92, 187, 148, 0.05));
  background: linear-gradient(0deg, #267656, rgba(92, 187, 148, 0.05));
  font-family: Source Han Sans CN;
}
.wallet_bg .wallet_bg_card {
    position: absolute;
    top: 22rpx;
    width: 100%;
}
.wallet_bg .wallet_bg_card .wallet_box_bg {
      width: 90%;
      margin: 0rpx auto;
      border-radius: 10px;
      height: 400rpx;
      background: #FFFFFF;
      overflow: hidden;
}
.wallet_bg .wallet_bg_card .wallet_box_bg .price_title {
        font-size: 28rpx;
        font-weight: 500;
        color: #555555;
        margin-top: 104rpx;
        -webkit-box-pack: center;
        -webkit-justify-content: center;
            -ms-flex-pack: center;
                justify-content: center;
}
.wallet_bg .wallet_bg_card .wallet_box_bg .price_none {
        text-align: center;
        font-size: 64rpx;
        color: #267656;
        margin-top: 20rpx;
}
.wallet_bg .wallet_bg_card .wallet_box_bg .yesterday_none {
        width: 260rpx;
        height: 46rpx;
        line-height: 46rpx;
        margin: 0 auto;
        background: #f6f5f5;
        font-size: 26rpx;
        color: #555555;
        border-radius: 22px;
        -webkit-box-pack: center;
        -webkit-justify-content: center;
            -ms-flex-pack: center;
                justify-content: center;
}
.wallet_bg .wallet_bg_card .wallet_box_bg .yesterday_none text {
          font-size: 44rpx;
          margin-top: 10rpx;
          color: #267656;
}
.wallet_bg .wallet_bg_card .wallet_box_bg .share_title {
        text-align: center;
        font-size: 24rpx;
        color: #555555;
        margin-top: 23rpx;
}
.my_index_box {
  width: 90%;
  margin: 60rpx auto;
  font-family: Source Han Sans CN;
  color: #555555;
}
.my_index_box .list_entrance {
    padding: 37rpx 31rpx;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    margin-top: 30rpx;
}
.my_index_box .list_entrance .icon_img {
      width: 43rpx;
      height: 35rpx;
      background: #007AFF;
}
.my_index_box .list_entrance .icon_title {
      font-size: 28rpx;
      font-weight: 400;
      margin-left: 26rpx;
}
.my_index_box .list_entrance .right_price {
      -webkit-box-align: center;
      -webkit-align-items: center;
          -ms-flex-align: center;
              align-items: center;
}
.my_index_box .list_entrance .right_price .price {
        margin-right: 10rpx;
        font-size: 32rpx;
        font-weight: 500;
}
.my_index_box .details_card {
    background: #fff;
    border-radius: 20rpx;
}
.bottom_tiantian {
